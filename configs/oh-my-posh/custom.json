{
  "$schema": "https://raw.githubusercontent.com/JanDeDobbeleer/oh-my-posh/main/themes/schema.json",
  "blocks": [
    {
      "type": "prompt",
      "alignment": "left",
      "segments": [
        {
          "options": {
            "display_root": true,
            "max_depth": 1,
            "style": "agnoster_short"
          },
          "foreground": "#d3869b",
          "type": "path",
          "style": "powerline"
        },
        {
          "options": {
            "branch_icon": "",
            "fetch_status": true,
            "source": "cli"
          },
          "template": "<#665c54>(</>{{ .HEAD }}<#665c54>)</> ",
          "foreground": "#83a598",
          "type": "git",
          "style": "powerline"
        }
      ]
    },
    {
      "type": "prompt",
      "alignment": "left",
      "segments": [
        {
          "template": "<#655c54>(</>{{ .Venv }}<#655c54>)</> ",
          "foreground": "#fabd2f",
          "type": "python",
          "style": "powerline"
        },
        {
          "template": "{{ if gt .Code 0 }}<#fb4934>{{ else }}</>{{ end }}\u203a",
          "foreground": "#ddc7a1",
          "type": "text",
          "style": "plain"
        }
      ]
    }
  ],
  "version": 4,
  "final_space": true
}
