{
  "$schema": "https://raw.githubusercontent.com/JanDeDobbeleer/oh-my-posh/main/themes/schema.json",
  "blocks": [
    {
      "type": "prompt",
      "alignment": "left",
      "segments": [
        {
          "type": "path",
          "style": "powerline",
          "foreground": "#d3869b",
          "properties": {
            "style": "agnoster_short",
            "max_depth": 1,
            "display_root": true
          }
        },
        {
          "type": "git",
          "style": "powerline",
          "foreground": "#83a598",
          "properties": {
            "fetch_status": true,
            "branch_icon": "",
            "source": "cli"
          },
          "template": "<#665c54>(</>{{ .HEAD }}<#665c54>)</> "
        }
      ]
    },
    {
      "type": "prompt",
      "alignment": "left",
      "segments": [
        {
          "type": "python",
          "style": "powerline",
          "foreground": "#fabd2f",
          "template": "<#655c54>(</>{{ .Venv }}<#655c54>)</> "
        },
        {
          "type": "text",
          "style": "plain",
          "foreground": "#ddc7a1",
          "template": "{{ if gt .Code 0 }}<#fb4934>{{ else }}</>{{ end }}\u203a"
        }
      ],
      "newline": false 
    }
  ],
  "final_space": true,
  "version": 3
}
