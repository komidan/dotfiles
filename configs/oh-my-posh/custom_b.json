{
  "$schema": "https://raw.githubusercontent.com/JanDeDobbeleer/oh-my-posh/main/themes/schema.json",
  "blocks": [
    {
      "type": "prompt",
      "alignment": "left",
      "segments": [
        {
          "options": {
            "cache_duration": "none"
          },
          "template": "{{ if .WSL }}<#e8c98b>wsl</> {{ end }}<#6c6b6e></>{{ .UserName }}@{{ .HostName }}",
          "foreground": "#f3f2c3",
          "type": "session",
          "style": "plain"
        },
        {
          "options": {
            "cache_duration": "none",
            "folder_icon": "..",
            "home_icon": "~",
            "style": "agnoster_short"
          },
          "template": " {{ .Path }}<#6c6b6e></> ",
          "foreground": "#ceaaf0",
          "type": "path",
          "style": "plain"
        },
        {
          "options": {
            "branch_icon": "",
            "cache_duration": "none"
          },
          "template": "<#525252>(<#5f4fd1>{{ .HEAD }}<#525252>) </>",
          "foreground": "#748ed6",
          "type": "git",
          "style": "plain"
        }
      ]
    },
    {
      "type": "prompt",
      "alignment": "left",
      "segments": [
        {
          "options": {
            "cache_duration": "none"
          },
          "template": "<#5f5063>({{ if .Error }}{{ .Error }}{{ else }}{{ .Full }}{{ end }}) </>",
          "type": "python",
          "style": "powerline"
        },
        {
          "options": {
            "cache_duration": "none"
          },
          "template": "{{ if gt .Code 0 }}<#e66a7a>{{ else }}</>{{ end }}\u203a",
          "foreground": "#FFFFFFFF",
          "type": "text",
          "style": "plain"
        }
      ]
    }
  ],
  "version": 4,
  "final_space": true
}
